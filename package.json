    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-lodash": "^1.10.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^1.3.0",
    "gulp-eslint": "^3.0.1",
    "gulp-notify": "^2.2.0",
