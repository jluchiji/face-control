{
  "root": true,
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "new-cap": 0,
    "no-undef": 2,
    "no-shadow": 1,
    "id-length": 0,
    "func-names": 0,
    "no-console": 1,
    "key-spacing": 0,
    "comma-dangle": [
      1,
      "never"
    ],
    "no-loop-func": 1,
    "padded-blocks": 0,
    "no-unused-vars": 1,
    "prefer-template": 1,
    "no-multi-spaces": 0,
    "no-const-assign": 2,
    "no-param-reassign": 0,
    "no-confusing-arrow": 1,
    "object-curly-spacing": [
      2,
      "always"
    ],
    "array-bracket-spacing": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      2,
      "never"
    ]
  },
  "ecmaFeatures": {
    "generators": true
  }
}
